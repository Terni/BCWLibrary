<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="BitcoinWallet.Layers.Views.VShops"
            x:Name="VShopsTabbedPage"
            Title="{Binding title}"
            BarTextColor="Black"
            BackgroundColor="White"
>

    <TabbedPage.Children>
        <ContentPage Title="ATMs and Shops..." x:Name="atmsAndShops" >
            <StackLayout Spacing="20" Padding="20"  HorizontalOptions="StartAndExpand">
                <Label Text="All ATMs and Shops on the World." TextColor="Black"/>
            </StackLayout>
        </ContentPage>

        <ContentPage Title="Geocode Position..." x:Name="geocode" >
            <StackLayout Spacing="20" Padding="20" HorizontalOptions="StartAndExpand">
                <Label Text="Me Geocode." TextColor="Black"/>
            </StackLayout>
        </ContentPage>

    </TabbedPage.Children>
</TabbedPage>